<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="540.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.atvlpi3.Controllers.FelineShowController">
   <children>
      <Text layoutX="152.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Felino">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Text>
      <Label fx:id="speciesLabel" layoutX="80.0" layoutY="96.0" text="Label">
         <font>
            <Font name="Arial Rounded MT Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="habitatLabel" layoutX="80.0" layoutY="163.0" text="Label">
         <font>
            <Font name="Arial Rounded MT Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="weightLabel" layoutX="80.0" layoutY="233.0" text="Label">
         <font>
            <Font name="Arial Rounded MT Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="makeFelineSound" layoutX="76.0" layoutY="325.0" mnemonicParsing="false" onAction="#makeSound" prefHeight="25.0" prefWidth="80.0" text="Fazer som" />
      <Button fx:id="btnFelineMove" layoutX="76.0" layoutY="380.0" mnemonicParsing="false" onAction="#move" prefHeight="25.0" prefWidth="80.0" text="Mover" />
      <Button fx:id="btnFelineFeed" layoutX="76.0" layoutY="439.0" mnemonicParsing="false" onAction="#feed" prefHeight="25.0" prefWidth="80.0" text="Alimentar" />
